{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "defaultSeverity": "error",
    "rules": {
        "no-console":{
            "severity": "warning",
            "options": [
                "log",
                "error",
                "warn",
                "trace",
                "info",
                "debug"
            ]
        }
    }
}